<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
    <Import Project="libsilk-version.props" Condition=" '$(libsilk_VersionImported)' == '' "/>
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <libsilkLibDir>$(SolutionDir)libs\libsilk-$(libsilk_Version)\src</libsilkLibDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(libsilkLibDir)\..\interface;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>